{
  "name": "ppt-museum",
  "description": "ppt and pptx file uploader and slideshow",
  "main": "index.js",
  "authors": [
    "kaakaa"
  ],
  "license": "MIT",
  "keywords": [
    "ppt-museum"
  ],
  "homepage": "https://github.com/kaakaa/ppt-museum-webapp",
  "moduleType": [],
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "^3.3.6",
    "bootstrap-validator": "^0.10.1",
    "pdfjs-dist": "^1.4.135"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap-theme.min.css",
        "dist/fonts/*",
        "dist/js/bootstrap.min.js"
      ]
    },
    "bootstrap-validator": {
      "main": ["dist/validator.min.js"]
